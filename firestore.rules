
rules_version = '2';

service cloud.firestore {
  match /databases/{database}/documents {
    // Allow read and write access to all documents
    // for prototyping purposes.
    // WARNING: This is not secure for a production application.
    // You should implement more restrictive rules before going to production.
    match /{document=**} {
      allow read, write: if true;
    }
  }
}
